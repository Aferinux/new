          "_name": "extTabs15",
          "_customHTML": "<section class=\"tabs1\" group=\"Pricing Tables\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input inline type=\"range\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n        <input inline type=\"range\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n        <input type=\"checkbox\" title=\"Show Tabs\" name=\"showTabs\">\n        <input type=\"checkbox\" title=\"Show Styles Tabs\" name=\"showStyles\" condition=\"showTabs\" checked>\n        <input type=\"color\" title=\"Tabs Color\" name=\"tabsColor\" value=\"#66458e\" condition=\"showStyles && showTabs\">\n        <input type=\"color\" title=\"Tab Border\" name=\"tabsBorder\" value=\"#66458e\" condition=\"showStyles && showTabs\">\n        <input type=\"color\" title=\"Tab Bg Active\" name=\"tabsBg\" value=\"#66458e\" condition=\"showStyles && showTabs\">\n        <input type=\"checkbox\" title=\"Show Titles\" name=\"showTitles\" checked>\n        <input type=\"checkbox\" title=\"Show Subtitles\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Show Price Term\" name=\"showPriceTerm\" checked>\n        <input type=\"color\" title=\"Price Term Bg\" name=\"termBg\" value=\"#ffffff\" condition=\"showPriceTerm\">\n        <input type=\"color\" title=\"Lines Color\" name=\"lineColor\" value=\"#eaeaea\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked>\n        <input type=\"color\" title=\"Card Color\" name=\"cardColor\" value=\"#35086b\">\n        <input type=\"range\" inline title=\"Card Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\">\n        \n        <select title=\"Items Count\" name=\"itemsCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\" selected parallax>\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Gradient\" name=\"gradientBg\" condition=\"bg.type == 'color'\">\n        <input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#21b968\" condition=\"bg.type=='color'&&gradientBg\" selected>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#eff0ff\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay&&bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <ul class=\"nav nav-tabs mb-4 justify-content-center mx-auto\" role=\"tablist\" mbr-class=\"{'d-flex': showTabs}\" mbr-if=\"showTabs\">\n            <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item first mbr-section-btn\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrBtnColor,-mbrLink,-mbrBtnRemove\"><a class=\"btn mbr-fonts-style btn-bgr show btn-black-outline active\" role=\"tab\" data-toggle=\"tab\" href=\"#tabs1-0_tab0\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">Yearly (save 20%)</a></li>\n            <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item mbr-section-btn\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrBtnColor,-mbrLink,-mbrBtnRemove\"><a class=\"btn mbr-fonts-style btn-bgr show btn-black-outline active\" role=\"tab\" data-toggle=\"tab\" href=\"#tabs1-0_tab1\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">Monthly</a></li>\n        </ul>\n        <div class=\"tab-content\">\n            <div id=\"tab1\" class=\"tab-pane in active\" role=\"tabpanel\">\n                <div class=\"row align-items-start\">\n                    <div class=\"col-md-6 justify-content-center\" mbr-if=\"itemsCount > 0\" mbr-class=\"{\n                         'col-lg-4': itemsCount == 3,\n                         'col-lg-6': itemsCount == 2,\n                         'col-lg-3': itemsCount == 4}\">\n                        <div class=\"plan\">\n                            <div class=\"mbr-overlay\"></div>\n                            <div class=\"plan-header text-center\">\n                                <h3 class=\"plan-title mbr-white mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".plan-title\">\n                                    Investment Program 1</h3>\n                                <h4 class=\"plan-subtitle mbr-white mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".plan-subtitle\">\n                                    A beautiful. simple website\n                                </h4>\n                                <div class=\"plan-price\">\n                                    <span mbr-text class=\"price-figure mbr-white mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".plan-price .price-figure\">\n                                        $10,000</span>\n                                    <div class=\"price-term mbr-black\" mbr-if=\"showPriceTerm\">\n                                        <div class=\"term mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-text data-app-selector=\".price-term\">Minimum Investment</div>\n                                    </div>\n                                    <p class=\"mbr-text mbr-fonts-style \" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">Investment Program 1 is a very profitable strategy based in Forex fundamental analysis for trading the News. This News strategy has several months of high profits annually with a controlled low Drawdown.<br><br><br>\n                                    </p>\n                                </div>\n                            </div>\n                            <div class=\"plan-body\">\n                                <div class=\"plan-list align-center\">\n                                    <ul mbr-text class=\"list-group list-group-flush mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-btn, .list-group\">\n                                        <li class=\"list-group-item\">Asset Class: Major Currencies</li>\n                                        <li class=\"list-group-item\">Performance fee:50%</li>\n                                        <li class=\"list-group-item\">Platform: MT4</li>\n                                    </ul>\n                                </div>\n                                <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn align-center pt-4\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a href=\"page9.html\" class=\"btn btn-black btn-bgr\" data-app-placeholder=\"Type Text\">Invest Now</a></div>\n                            </div>\n                        </div>\n                    </div>\n\n                    <div class=\"col-md-6 justify-content-center\" mbr-if=\"itemsCount > 1\" mbr-class=\"{\n                         'col-lg-4': itemsCount == 3,\n                         'col-lg-6': itemsCount == 2,\n                         'col-lg-3': itemsCount == 4}\">\n                        <div class=\"plan\">\n                            <div class=\"mbr-overlay\"></div>\n                            <div class=\"plan-header text-center\">\n                                <h3 class=\"plan-title mbr-white mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".plan-title\">Investment Program 2</h3>\n                                <h4 class=\"plan-subtitle mbr-white mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".plan-subtitle\">\n                                    A beautiful. simple website\n                                </h4>\n                                <div class=\"plan-price\">\n                                    <span mbr-text class=\"price-figure mbr-white mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".plan-price .price-figure\">\n                                        $55,000</span>\n                                    <div class=\"price-term mbr-black\" mbr-if=\"showPriceTerm\">\n                                        <div class=\"term mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-text data-app-selector=\".price-term\">Minimum Investment</div>\n                                    </div>\n                                    <p class=\"mbr-text mbr-fonts-style \" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\" mbr-if=\"showText\">Investment program 2\n<br>objective is to provide net alpha returns and reduced volatility over the medium term through quantitative systematic forecasts.<br><br><br><br><br><br><br></p>\n                                </div>\n                            </div>\n                            <div class=\"plan-body\">\n                                <div class=\"plan-list align-center\">\n                                    <ul mbr-text class=\"list-group list-group-flush mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-btn, .list-group\">\n                                        <li class=\"list-group-item\">Annual Profit: 15.28%</li>\n                                        <li class=\"list-group-item\">Performance fee: 15%</li>\n                                        <li class=\"list-group-item\">\n                                            Sharp ratio: 1.87</li>\n                                    </ul>\n                                </div>\n                                <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn align-center pt-4\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a href=\"page9.html\" class=\"btn btn-black btn-bgr\" data-app-placeholder=\"Type Text\">Invest Now</a></div>\n                            </div>\n                        </div>\n                    </div>\n\n                    <div class=\"col-md-6 justify-content-center\" mbr-if=\"itemsCount > 2\" mbr-class=\"{\n                         'col-lg-4': itemsCount == 3,\n                         'col-lg-6': itemsCount == 2,\n                         'col-lg-3': itemsCount == 4}\">\n                        <div class=\"plan\">\n                            <div class=\"mbr-overlay\"></div>\n                            <div class=\"plan-header text-center\">\n                                <h3 class=\"plan-title mbr-white mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".plan-title\">Investment Program 3</h3>\n                                <h4 class=\"plan-subtitle mbr-white mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".plan-subtitle\">\n                                    A beautiful. simple website\n                                </h4>\n                                <div class=\"plan-price\">\n                                    <span mbr-text class=\"price-figure mbr-white mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".plan-price .price-figure\">\n                    